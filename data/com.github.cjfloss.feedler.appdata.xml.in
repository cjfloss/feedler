  <description>
    <p>Feedler is a lightweight news reading software with RSS and ATOM support.</p>
    <p>Originaly made by Daniel Kur.</p>
  </description>

  <launchable type="desktop-id">com.github.cjfloss.envelope.desktop</launchable>

  <provides>
    <binary>com.github.cjfloss.feedler</binary>
    <binary>com.github.cjfloss.feedler-service</binary>
    <dbus type="system">org.example.Feedler</dbus>
  </provides>

